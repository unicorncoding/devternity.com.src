  script.
    /* Add a basic data series with six labels and values */
    var data = {
      labels: ['not a fellow', '1 attendee', '2 attendees', '5 attendees', '10 attendees', '20 attendees'],
      series: [ { data: [0, 50, 100, 250, 500, 1000] } ]
    };
    var options = {
      height: 400,
      axisY: { offset: 100, labelInterpolationFnc: function(value) { return value + ' EUR' } }      
    };      
    new Chartist.Line('.ct-chart', data, options);   

    var loginBtn = $('#authenticate');
    var logoutBtn = $('#logout');
    var stepAuth = $('#step-auth');
    var stepLink = $('#step-link');
    var uniqueLink = $('#uniqueLink');
    var profile = $('#profile')

    function isAuthenticated() {
      var expiresAt = JSON.parse(localStorage.getItem('expires_at'));
      return new Date().getTime() < expiresAt;
    }

    function setSession(authResult) {
      var expiresAt = JSON.stringify(
        authResult.expiresIn * 1000 + new Date().getTime()
      );
      localStorage.setItem('access_token', authResult.accessToken);
      localStorage.setItem('id_token', authResult.idToken);
      localStorage.setItem('expires_at', expiresAt);
      localStorage.setItem('email', authResult.idTokenPayload.email);
      localStorage.setItem('name', authResult.idTokenPayload.given_name);
    }

    function logout() {
      localStorage.removeItem('access_token');
      localStorage.removeItem('id_token');
      localStorage.removeItem('expires_at');
      displayButtons();
    }    

    function theLink() {
      return 'https://devternity.com?r=' + window.btoa(localStorage.getItem('email'));
    }

   function displayButtons() {
      if (isAuthenticated()) {
        $("#fb").attr('href', 'https://www.facebook.com/sharer/sharer.php?u=' + theLink() + '&quote=The most waited tech conference of the year is coming. Are you in? @devternity')
        $("#tw").attr('href', 'https://twitter.com/intent/tweet?text=The most waited tech conference of the year is coming. Are you in?&hashtags=techconference,tech,technology,devternity&url=' + theLink());
        $("#gp").attr('href', 'https://plus.google.com/share?url=' + theLink());
        $("#ln").attr('href', 'https://www.linkedin.com/shareArticle?mini=true&url=' + theLink() + '&title=DevTernity is coming! &summary=The most waited tech conference of the year is coming. Are you in?&source=');
        $('.anonym-only').addClass('is-hidden');
        $('.logged-only').removeClass('is-hidden');
        $('.step-item').addClass('is-completed');
        profile.text('Hey ' + localStorage.getItem('name') + '!')
        uniqueLink.text(theLink())
      } else {
        $('.step-item').removeClass('is-completed');               
        $('.anonym-only').removeClass('is-hidden');
        $('.logged-only').addClass('is-hidden');
      
      }
    }    

    $('document').ready(function() {

      var webAuth = new auth0.WebAuth({
        domain: 'devternity.eu.auth0.com',
        clientID: '6WYcIyMhyVsPp2h6YfCPZ-3kjARNzazu',
        responseType: 'token id_token',
        audience: 'https://devternity.eu.auth0.com/userinfo',
        scope: 'openid email profile',
        redirectUri: window.location.href
      });

      loginBtn.click(function(e) {
        e.preventDefault();
        webAuth.authorize();
      });

      logoutBtn.click(logout);



      webAuth.parseHash(function(err, authResult) {
        if (authResult && authResult.accessToken && authResult.idToken) {
          window.location.hash = '';
          setSession(authResult);
          console.log(authResult);
        }
        displayButtons();
      });      

    });

